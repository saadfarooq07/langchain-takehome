# API Keys
GEMINI_API_KEY=your_google_api_key_here
GROQ_API_KEY=your_groq_api_key_here
TAVILY_API_KEY=your_tavily_api_key_here
LANGSMITH_API_KEY=your_langsmith_api_key_here

# LangSmith Configuration
# Set to empty string to use no prefix (just 'main', 'validation', etc.)
# Set to your organization name to use org prefix (e.g., 'myorg' -> 'myorg/main')
# Default: 'log-analyzer'
LANGSMITH_PROMPT_PREFIX=

# PostgreSQL Database
DATABASE_URL=postgresql://loganalyzer:password@localhost:5432/loganalyzer
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=loganalyzer
POSTGRES_USER=loganalyzer
POSTGRES_PASSWORD=password

# Better Auth Configuration
BETTER_AUTH_URL=http://localhost:8000
BETTER_AUTH_SECRET=your_secret_key_here_min_32_chars

# Optional Configuration
MAX_LOOPS=5
MAX_SEARCH_RESULTS=3

# Graph Iteration Limits (Phase 1 improvements)
MAX_ANALYSIS_ITERATIONS=10  # Maximum times to retry analysis
MAX_TOOL_CALLS=20          # Maximum tool calls allowed
MAX_VALIDATION_RETRIES=3   # Maximum validation retry attempts

# Cycle Detection Configuration
MAX_SIMPLE_LOOPS=3         # Maximum simple loops (A→B→A)
MAX_COMPLEX_LOOPS=2        # Maximum complex loops (A→B→C→A)
CYCLE_DETECTION_WINDOW=20  # Window size for pattern detection

# Improved Log Analyzer Implementation (Phase 2, 3, 4)
# USE_IMPROVED_LOG_ANALYZER=true  # Enable all improvements

# Feature Sets (choose one)
# LOG_ANALYZER_FEATURE_SET=minimal      # No extra features
# LOG_ANALYZER_FEATURE_SET=standard     # streaming, caching
# LOG_ANALYZER_FEATURE_SET=interactive  # + interactive
# LOG_ANALYZER_FEATURE_SET=memory       # + memory
# LOG_ANALYZER_FEATURE_SET=improved     # + specialized, circuit_breaker, rate_limiting (recommended)
# LOG_ANALYZER_FEATURE_SET=full         # All stable features
# LOG_ANALYZER_FEATURE_SET=experimental # + experimental features

# Individual Feature Toggles
# FEATURE_STREAMING_ENABLED=true         # Auto-streaming for large logs
# FEATURE_SPECIALIZED_ENABLED=true       # Domain-specific analyzers
# FEATURE_CIRCUIT_BREAKER_ENABLED=true   # Fault tolerance
# FEATURE_RATE_LIMITING_ENABLED=true     # API quota protection
# FEATURE_CACHING_ENABLED=true          # Result caching

# Feature Configuration
# FEATURE_STREAMING_CHUNK_SIZE_MB=10    # Chunk size for streaming
# FEATURE_CIRCUIT_BREAKER_FAILURE_THRESHOLD=5
# FEATURE_RATE_LIMITING_GEMINI_RPM=60   # Requests per minute